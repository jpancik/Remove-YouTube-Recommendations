{
"name":"Remove YouTube Recommendations",
"description":"Get rid of (usually) irrelevant recommendations from YouTube.",
"version":"4",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["remove-youtube-recommendations.js"]
    }
  ],
"icons": { "16": "icon16.png",
           "48": "icon48.png",
		   "128": "icon128.png" }
}